    "def hindex(citations):\n",
    "    citations.sort(reverse=True)\n",
    "    print(citations,\"cita\")\n",
    "    h = 0\n",
    "    for i, c in enumerate(citations):\n",
    "        if c >= i + 1:\n",
    "            h = i + 1\n",
    "        else:\n",
    "            break\n",
    "    return h\n",
    "\n",
    "citations = [3, 0, 6, 1, 5]\n",
    "\n",
    "print(hindex(citations))"
   ]
  }
